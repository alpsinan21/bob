{
  "params": {
    "NAME": {
    },
    "VERSION": {
    },
    "BOB_SRC_DIR": {
      "default": "."
    },
    "BOB_STYLE_BIN": {
      "default": "node_modules/jscheckstyle/bin/jscheckstyle"
    },
    "BOB_STYLE_FILES": {
      "default": "lib/"
    },
    "BOB_STYLE_OPTS": {
    },
    "BOB_LINT_BIN": {
      "default": "node_modules/jshint/bin/hint",
      "nodelint": "node_modules/nodelint/nodelint"
    },
    "BOB_LINT_FILES": {
      "default": "lib/ test/"
    },
    "BOB_LINT_OPTS": {
    },
    "BOB_TEST_BIN": {
      "default": "node_modules/vows/bin/vows"
    },
    "BOB_TEST_FILES": {
      "default": "`find test -name \"*.js\" | tr \"\\r\\n\" \" \"`"
    },
    "BOB_TEST_OPTS": {
    },
    "BOB_COVERAGE_BIN": {
      "default": "node_modules/expresso/deps/jscoverage/node-jscoverage"
    },
    "BOB_COVERAGE_TESTBIN": {
      "default": "node_modules/vows/bin/vows"
    },
    "BOB_COVERAGE_FILES": {
      "default": "`find test -name \"*.js\" | tr \"\\r\\n\" \" \"`"
    },
    "BOB_COVERAGE_OPTS": {
    },
    "BOB_DOC_BIN": {
      "default": "node_modules/ndoc/bin/ndoc"
    },
    "BOB_DOC_FILES": {
      "default": "lib/"
    },
    "BOB_DOC_OPTS": {
      "default": "-o build/report/doc"
    },
    "BOB_PACKAGEMETA_DIR": {
    },
    "BOB_PACKAGEMETA_FILE": {
    },
    "BOB_DEPLOY_USER": {
      "default": "anonymous"
    },
    "BOB_DEPLOY_KEY": {
    },
    "BOB_DEPLOY_HOST": {
    },
    "BOB_DEPLOY_PORT": {
      "default": "22",
      "ftp": "21"
    },
    "BOB_DEPLOY_DIR": {
      "default": ".",
      "ftp": "/"
    },
    "BOB_DEPLOY_BIN": {
      "default": "scp",
      "ftp": "ncftpput"
    },
    "BOB_DEPLOY_OPTS": {
      "default": "-i $(BOB_DEPLOY_KEY) -P $(BOB_DEPLOY_PORT)",
      "ftp": "-f $(BOB_DEPLOY_KEY) -u $(BOB_DEPLOY_USER) -P $(BOB_DEPLOY_PORT)"
    },
    "BOB_DEPLOY_FILES": {
      "default": "$(ARTIFACT_DIR)/* $(BOB_DEPLOY_USER)@$(BOB_DEPLOY_HOST):$(BOB_DEPLOY_DIR)",
      "ftp": "$(BOB_DEPLOY_DIR) $(ARTIFACT_DIR)/$(FULLNAME).tar.gz"
    }
  },
  "targets": {
    "restart": {
      "override": {
        "prop": "scripts.restart",
        "target": "restart-npm"
      }
    },
    "start": {
      "override": {
        "prop": "scripts.start",
        "target": "start-npm"
      }
    },
    "stop": {
      "override": {
        "prop": "scripts.stop",
        "target": "stop-npm"
      }
    }
  }
}
