{
  "default": "plato",
  "types": {
    "plato": {
      "bin": "{bob}/node_modules/plato/bin/plato",
      "opts": "--recurse --dir .bob/complexity/plato",
      "args": "lib"
    },
    "jscheckstyle": {
      "bin": "{bob}/node_modules/jscheckstyle/bin/jscheckstyle",
      "opts": {
