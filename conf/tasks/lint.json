{
  "default": "eslint",
  "types": {
    "eslint": {
      "bin": "{bob}/node_modules/.bin/eslint",
      "opts": {
        "human": "",
        "robot": ""
      },
      "args": "lib/ test/"
    },
    "jshint": {
      "bin": "{bob}/node_modules/.bin/jshint",
      "opts": {
        "human": "",
        "robot": "--jslint-reporter"
      },
      "args": "lib/ test/"
    },
    "nodelint": {
      "bin": "{bob}/node_modules/.bin/nodelint",
      "opts": {
        "human": "",
        "robot": "--reporter=xml"
      },
      "args": "lib/ test/"
    }
  }
}