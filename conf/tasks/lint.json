{
  "default": "jshint",
  "types": {
    "jshint": {
      "bin": "{bob}/node_modules/jshint/bin/jshint",
      "opts": {
        "human": "",
        "robot": "--jslint-reporter"
      },
      "args": "lib/ test/"
    },
    "nodelint": {
      "bin": "{bob}/node_modules/nodelint/nodelint",
      "opts": {
        "human": "",
        "robot": "--reporter=xml"
      },
      "args": "lib/ test/"
    }
  }
}