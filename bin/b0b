#! /bin/sh

if [ -f ./b0b.cfg ]
then
    . ./b0b.cfg
fi

make -f $B0B_HOME/Makefile \
    APP_NAME="$APP_NAME" \
    APP_VERSION="$APP_VERSION" \
    APP_DIR=`pwd` \
    APP_SRC_DIR="$APP_SRC_DIR" \
    BUILD_LINT_FILES="$BUILD_LINT_FILES" \
    DEPLOY_HOST="$DEPLOY_HOST" \
    DEPLOY_PORT="$DEPLOY_PORT" \
    DEPLOY_DIR="$DEPLOY_DIR" \
    $1 $2 $3 $4 $5 $6 $7 $8 $9
